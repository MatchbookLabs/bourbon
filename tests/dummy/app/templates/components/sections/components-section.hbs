    {{/freestyle-note}}
    {{#freestyle-usage 'open-accordion' title='open accordion' usageTitle='Usage'}}
      {{bourbon-accordion list=openAccordionList}}
    {{/freestyle-usage}}


  {{/section.subsection}}

{{/freestyle-section}}
