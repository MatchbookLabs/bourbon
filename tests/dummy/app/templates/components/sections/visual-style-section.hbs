{{#freestyle-section name='Visual Style' as |section|}}

  {{#section.subsection name='Typography'}}

    {{#freestyle-usage 'typography-helvetica' title='Helvetica Neue'}}
      {{freestyle-typeface fontFamily='"Helvetica Neue", Arial, Helvetica, sans-serif'}}
    {{/freestyle-usage}}

    {{#freestyle-usage 'typography-open-sans' title='Open Sans'}}
      {{freestyle-typeface fontFamily='"Open Sans", Helvetica, Arial, sans-serif'}}
    {{/freestyle-usage}}

    {{#freestyle-usage 'typography-menlo' title='Menlo'}}
      {{freestyle-typeface fontFamily='Menlo, "Andale Mono", AndaleMono, Consolas, monaco, monospace'}}
    {{/freestyle-usage}}

    {{#freestyle-usage 'typography-roboto' title='Roboto'}}
      {{freestyle-typeface fontFamily='Roboto, "Open Sans", Helvetica, Arial, sans-serif'}}
    {{/freestyle-usage}}

  {{/section.subsection}}

  {{#section.subsection name='Color'}}
    {{#freestyle-usage 'fp' title='Getfeedback Palette' usageTitle='Usage'}}
      {{freestyle-palette
        colorPalette=colorPalette
        title='Getfeeback Color Palette'
        description='This component displays the color palette specified in freestyle/palette.json'
      }}
    {{/freestyle-usage}}
  {{/section.subsection}}

  
{{/freestyle-section}}
