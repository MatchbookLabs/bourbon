{{#if partial}}
  {{#popover-on-element class=tooltipBackgroundClass side=tooltipPosition event=tooltipEvent spacing=tooltipSpacing}}
    {{partial partial}}
  {{/popover-on-element}}
{{else}}
  {{#tooltip-on-element class=tooltipBackgroundClass side=tooltipPosition event=tooltipEvent spacing=tooltipSpacing}}
    {{text}}
  {{/tooltip-on-element}}
{{/if}}
